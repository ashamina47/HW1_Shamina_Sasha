
c = list(range(30, 65, 5))
In [5]:

print(c)
[30, 35, 40, 45, 50, 55, 60]
In [6]:

print(c[::-1])
[60, 55, 50, 45, 40, 35, 30]
In [7]:

d = c[::-1]
In [8]:

print(d)
[60, 55, 50, 45, 40, 35, 30]
In [9]:

d.insert(0, 65)
In [10]:

print(d)
[65, 60, 55, 50, 45, 40, 35, 30]
